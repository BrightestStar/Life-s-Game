<%= simple_form_for [:admin, @objective] do |f| %>

<p>
    <%= f.label :任务名称 %><br>
    <%= f.text_field :name %>
</p>

<p>
    <%= f.label :任务一 %><br>
    <%= f.text_area :task %>
</p>

<p>
    <%= f.label :任务二 %><br>
    <%= f.text_area :second %>
</p>

<p>
    <%= f.label :任务三 %><br>
    <%= f.text_area :third %>
</p>

<p>
    <%= f.submit "Submit", data: { disable_with: "Submitting..." } %>
</p>
<% end %>
